{
	"name": "@svapp/web",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean": "rm -rf node_modules .turbo .svelte-kit .eslintcache",
		"dev": "vite dev",
		"format:check": "prettier '**/*.{cjs,mjs,ts,md,json,svelte}' --ignore-path ../.gitignore --ignore-unknown --check",
		"format": "prettier '**/*.{cjs,mjs,ts,md,json,svelte}' --ignore-path ../.gitignore --ignore-unknown --write",
		"lint:fix": "pnpm lint --fix",
		"lint": "eslint . --report-unused-disable-directives",
		"preview": "vite preview",
		"start": "vite dev",
		"test:unit": "vitest",
		"test": "npm run test:integration && npm run test:unit",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.7",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.1.4",
		"vitest": "^1.3.1"
	}
}
